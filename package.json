{
  "name": "transbot",
  "description": "A transition goals tracker written in slashasarus",
  "private": "true",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && npm run login",
    "build": "tsc",
    "login": "node --enable-source-maps . | pino-pretty -i pid,hostname -t yyyy-mm-dd HH:MM:ss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/night-lake/transbot.git"
  },
  "bugs": {
    "url": "https://github.com/night-lake/transbot/issues"
  },
  "author": "Sadie Katherine",
  "license": "ISC",
  "homepage": "https://github.com/night-lake/transbot#readme",
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "pino": "^7.10.0",
    "pino-pretty": "^7.6.1",
    "slashasaurus": "^0.5.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prisma": "^3.12.0",
    "typescript": "^4.6.3"
  }
}
